package com.bookmyshow.model;

import java.util.List;

public class Movies {

	private String movieId;
	private String movieName;
	private long movieRating;
	private long movieLikes;
	private String imgUrl;
	private String movieYear;
	private long movieDuration;
	
	private List<String> movieCast;
	private List<String> movieGenre;
	private List<String> movieLanguage;
	
	private List<MovieLocation> movieLocation;

	
	/**
	 * 
	 */
	public Movies() {}

	public String getMovieId() {
		return movieId;
	}

	public void ListMovieId(String movieId) {
		this.movieId = movieId;
	}

	public String getMovieName() {
		return movieName;
	}

	public void ListMovieName(String movieName) {
		this.movieName = movieName;
	}

	public long getMovieRating() {
		return movieRating;
	}

	public void ListMovieRating(long movieRating) {
		this.movieRating = movieRating;
	}

	public long getMovieLikes() {
		return movieLikes;
	}

	public void ListMovieLikes(long movieLikes) {
		this.movieLikes = movieLikes;
	}

	public String getImgUrl() {
		return imgUrl;
	}

	public void ListImgUrl(String imgUrl) {
		this.imgUrl = imgUrl;
	}

	public String getMovieYear() {
		return movieYear;
	}

	public void ListMovieYear(String movieYear) {
		this.movieYear = movieYear;
	}

	public long getMovieDuration() {
		return movieDuration;
	}

	public void ListMovieDuration(long movieDuration) {
		this.movieDuration = movieDuration;
	}

	public List<String> getMovieCast() {
		return movieCast;
	}

	public void ListMovieCast(List<String> movieCast) {
		this.movieCast = movieCast;
	}

	public List<String> getMovieGenre() {
		return movieGenre;
	}

	public void ListMovieGenre(List<String> movieGenre) {
		this.movieGenre = movieGenre;
	}

	public List<String> getMovieLanguage() {
		return movieLanguage;
	}

	public void ListMovieLanguage(List<String> movieLanguage) {
		this.movieLanguage = movieLanguage;
	}

	public List<MovieLocation> getMovieLocation() {
		return movieLocation;
	}

	public void ListMovieLocation(List<MovieLocation> movieLocation) {
		this.movieLocation = movieLocation;
	}
	
	
	
	
	
}
